          "\t${1:NewRoute}.DY<meas> = ${3:0}",
          "\t${1:NewRoute}.DZ<meas> = ${4::DZ}",
          "\t${1:NewRoute}.X<meas> = ${5:0}",
          "\t${1:NewRoute}.Y<meas> = ${6:0}",
          "\t${1:NewRoute}.Z<meas> = ${7:0}",

{
  "NewPart": {
    "prefix": "NewPart",
    "body": [
        "const ${1:NewPart} = _this.CreateChild(OBJ_PART, '${2:part name}'${3:, string description[optional]});",
        "${1:NewPart}.DX = ${4:0};",
        "${1:NewPart}.DY = ${5:0};",
        "${1:NewPart}.DZ = ${6:'_M:DZ'};",
        "${1:NewPart}.X = ${7:0};",
        "${1:NewPart}.Y = ${8:0};",
        "${1:NewPart}.Z = ${9:0};",
        "${1:NewPart}.AX = ${10:0};",
        "${1:NewPart}.AY = ${11:0};",
        "${1:NewPart}.AZ = ${12:0};"
    ],
    "description": "Creates a new Part with customizable name"
  },
